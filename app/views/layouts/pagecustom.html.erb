<%= render 'layouts/menubar' %>
<%= render 'layouts/selected_posts' %>

<%# Code Below Sorts out Page titles%>
<% if @page.name != "Homepage" %>
<% provide(:title, @page.name + "|" + @app_name) %>
<% elsif @page.name == "Homepage" %>
<% provide(:title, @app_name) %>
<% end %>

<%=  Liquid::Template.parse(@page.layout.custom_layout).render('page_content' => yield, 'page_title' => yield(:title), 'appname' => @app_name, 'widget1' => @widget1, 'widget2'=> @widget2, 'widget3' => @widget3, 'widget4' => @widget4, 'widget5' => @widget5, 'widget6' => @widget6, 'widget7' => @widget7, 'widget8' => @widget8, 'widget9' => @widget9, 'widget10' => @widget10, "menu_items" => @menu_list_items,  "selected_posts" => @array_selected_posts
 ).html_safe %>
      
